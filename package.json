    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015-webpack": "^6.4.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "css-loader": "^0.23.1",
    "eslint": "^2.8.0",
    "eslint-config-airbnb-base": "^1.0.3",
    "eslint-plugin-babel": "^3.2.0",
